  "components": {
    "securitySchemes": {
      "userSessionKey": {
        "type": "apiKey",
        "in": "cookie",
        "name": "user_session_key",
        "description": "User session key cookie"
      },
      "chillipharmSession": {
        "type": "apiKey",
        "in": "cookie",
        "name": "_chillipharm_session",
        "description": "ChilliPharm session cookie"
      }
    },
    "schemas": {
      "AssetWithInfoFields": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "description": "The asset id",
            "example": 1
          },
          "title": {
            "type": "string",
            "description": "The asset title",
            "example": "Asset 1"
          },
          "filename": {
            "type": "string",
            "description": "The asset filename",
            "example": "asset_1.mp4"
          },
          "extension": {
            "type": "string",
            "description": "The asset extension",
            "example": "mp4"
          },
          "created_at": {
            "type": "string",
            "description": "The asset created at date",
            "example": "2020-01-01T00:00:00.000Z"
          },
          "created_by": {
            "$ref": "#/components/schemas/User"
          },
          "media_type": {
            "type": "string",
            "description": "The asset media type",
            "example": "video"
          },
          "media": {
            "oneOf": [
              {
                "type": "object",
                "description": "The links to the asset media",
                "properties": {
                  "url": {
                    "type": "string",
                    "description": "The asset media url",
                    "nullable": true,
                    "example": "https://eu-west-4-chillipharm-bucket.s3.eu-west-4.amazonaws.com/test_data/Image.png"
                  }
                },
                "additionalProperties": false,
                "required": [
                  "url"
                ]
              },
              {
                "type": "object",
                "description": "The links to the asset media",
                "properties": {
                  "sd_video": {
                    "$ref": "#/components/schemas/Video"
                  },
                  "hd_video": {
                    "$ref": "#/components/schemas/Video"
                  }
                },
                "additionalProperties": false,
                "required": [
                  "sd_video",
                  "hd_video"
                ]
              }
            ]
          },
          "asset_infos": {
            "$ref": "#/components/schemas/InfoFields"
          },
          "thumbnails": {
            "$ref": "#/components/schemas/Thumbnails"
          },
          "study_procedure_identifier": {
            "type": "string",
            "description": "The asset study procedure identifier",
            "example": "PROCEDURE"
          },
          "study_event_identifier": {
            "type": "string",
            "description": "The asset study event identifier",
            "example": "EVENT"
          },
          "subject_number": {
            "type": "string",
            "description": "The asset subject number",
            "example": "123"
          },
          "chapters": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Chapter"
            }
          },
          "position": {
            "type": "number",
            "description": "The position of asset",
            "example": 1
          },
          "processed": {
            "type": "boolean",
            "description": "The asset processed status",
            "example": true
          }
        },
        "additionalProperties": false,
        "required": [
          "id",
          "title",
          "filename",
          "extension",
          "created_at",
          "created_by",
          "media_type",
          "media",
          "asset_infos",
          "thumbnails",
          "processed"
        ]
      },
      "AuthenticityToken": {
        "description": "The authenticity token",
        "type": "string",
        "example": "valid_token"
      },
      "Chapter": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "The id of chapter",
            "example": "c192a32d-c5cf-483d-8a30-2a0def1290d3"
          },
          "chapter_definition_id": {
            "type": "integer",
            "description": "The chapter definition id",
            "example": 1,
            "nullable": true
          },
          "name": {
            "type": "string",
            "description": "The chapter name",
            "example": "Chapter 1"
          },
          "color": {
            "type": "string",
            "description": "The chapter color",
            "example": "#000000"
          },
          "start_time": {
            "type": "integer",
            "description": "The chapter start time",
            "example": 0
          },
          "end_time": {
            "type": "integer",
            "description": "The chapter end time",
            "example": 100
          }
        },
        "additionalProperties": false,
        "required": [
          "id",
          "name",
          "color",
          "start_time",
          "end_time"
        ]
      },
      "Error": {
        "description": "A legacy error",
        "type": "object",
        "properties": {
          "error": {
            "type": "string",
            "description": "The error message",
            "example": "Bad request. Missing or invalid input parameters."
          }
        },
        "additionalProperties": false,
        "required": [
          "error"
        ]
      },
      "FormTemplateField": {
        "description": "Form template field",
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "The field id",
            "example": "1"
          },
          "title": {
            "type": "string",
            "description": "The field title",
            "example": "Field title"
          },
          "type": {
            "type": "string",
            "description": "The field type",
            "example": "single_line"
          },
          "input_guidance": {
            "type": "string",
            "description": "The field input guidance",
            "example": "guidance",
            "nullable": true
          },
          "enabled": {
            "type": "boolean",
            "description": "Whether the field is enabled",
            "example": true
          },
          "other_enabled": {
            "type": "boolean",
            "description": "Whether other types are enabled",
            "example": false
          },
          "value": {
            "oneOf": [
              {
                "type": "string",
                "example": ""
              },
              {
                "type": "array",
                "description": "The field value",
                "example": []
              }
            ]
          },
          "validations": {
            "type": "object",
            "properties": {
              "required": {
                "type": "boolean",
                "description": "Whether the field is required",
                "example": true
              },
              "input_format": {
                "type": "string",
                "description": "regex format for input",
                "example": "[a-zA-Z]{1}[a-zA-Z]{1}-\\d{1}\\d{1}\\d{1}\\d{1}\\d{1}",
                "nullable": true
              },
              "character_limit": {
                "type": "integer",
                "description": "The maximum characters",
                "example": 10,
                "nullable": true
              },
              "character_restriction": {
                "type": "string",
                "description": "restricted character type",
                "example": "letters_and_numbers",
                "nullable": true
              }
            },
            "required": [
              "required",
              "input_format",
              "character_limit",
              "character_restriction"
            ],
            "additionalProperties": false
          },
          "options": {
            "type": "array",
            "description": "The field options",
            "items": {
              "type": "string",
              "example": "option1"
            }
