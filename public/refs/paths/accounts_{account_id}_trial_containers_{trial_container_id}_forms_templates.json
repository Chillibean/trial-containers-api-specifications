{
  "get": {
    "summary": "Get Form Templates",
    "description": "Retrieves a list of form templates for the specified trial container.",
    "tags": [
      "Trial Container Forms Templates"
    ],
    "parameters": [
      {
        "name": "account_id",
        "in": "path",
        "required": true,
        "description": "The ID of the account.",
        "schema": {
          "type": "string"
        }
      },
      {
        "name": "trial_container_id",
        "in": "path",
        "required": true,
        "description": "The ID of the trial container.",
        "schema": {
          "type": "string"
        }
      },
      {
        "name": "form_container_id",
        "in": "query",
        "required": false,
        "description": "The ID of the form container used to filter templates.",
        "schema": {
          "type": "string"
        }
      },
      {
        "name": "form_container_type",
        "in": "query",
        "required": false,
        "description": "The type of form container used to filter templates.",
        "schema": {
          "type": "string"
        }
      }
    ],
    "responses": {
      "200": {
        "description": "A list of form templates.",
        "content": {
          "application/json": {
            "schema": {
              "type": "array",
              "items": {
                "$ref": "../components/schemas/FormTemplate.json"
              }
            }
          }
        }
      }
    }
  }
}