{
  "type": "object",
  "description": "The signature",
  "properties": {
    "created_at": {
      "type": "string",
      "description": "The signature created at",
      "example": "2025-01-15T12:00:00.000Z"
    },
    "email": {
      "type": "string",
      "description": "The signature email",
      "example": "johndoe@chillipharm.com"
    },
    "first_name": {
      "type": "string",
      "description": "The signature first name",
      "example": "John"
    },
    "id": {
      "type": "string",
      "description": "The signature uuid",
      "example": "0bb4fa28-9e55-4d06-8eb4-3c59e0030e7f"
    },
    "last_name": {
      "type": "string",
      "description": "The signature last name",
      "example": "Doe"
    },
    "meaning": {
      "type": "string",
      "description": "The signature meaning",
      "example": "Submit"
    },
    "object_uuid": {
      "type": "string",
      "description": "The signature object uuid",
      "example": "0bb4fa28-9e55-4d06-8eb4-3c59e0030e7f"
    },
    "reason": {
      "type": "string",
      "description": "The signature reason",
      "example": "I am the owner of this document"
    },
    "user_uuid": {
      "type": "string",
      "description": "The signature user uuid",
      "example": "0bb4fa28-9e55-4d06-8eb4-3c59e0030e7f"
    },
    "verification_status": {
      "type": "string",
      "description": "The signature verification status",
      "example": "verified"
    }
  },
  "required": [
    "created_at",
    "email",
    "first_name",
    "id",
    "last_name",
    "meaning",
    "object_uuid",
    "reason",
    "user_uuid",
    "verification_status"
  ],
  "additionalProperties": false
}
